{
  "name": "tsd2cce",
  "version": "0.1.1",
  "description": "Converter to generate google closure compiler extern files from typescript declaration files (.d.ts).",
  "bugs": {
    "url": "https://github.com/eredo/tsd2cce/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eredo/tsd2cce.git"
  },
  "main": "index.js",
  "directories": {
    "test": "test",
    "bin": "bin"
  },
  "bin": {
    "tsd2cee": "./bin/tsd2cee"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "js-beautify": "^1.7.5",
    "optimist": "^0.6.1",
    "typescript": "^1.8.10"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "eslint": "^5.1.0",
    "mocha": "^2.3.4"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha test/",
    "lint": "./node_modules/.bin/eslint -c ./.eslintrc src/**/*.js"
  },
  "keywords": [
    "google",
    "closure",
    "compiler",
    "closure",
    "compiler",
    "typescript",
    "typescript",
    "declaration",
    "closure",
    "externs",
    "externs",
    "d.ts",
    "tsd"
  ],
  "author": "Eric Schneller <eric@schnellers.name>",
  "license": "MIT"
}
